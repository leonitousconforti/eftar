{
    "$schema": "./node_modules/@effect/docgen/schema.json",
    "exclude": ["src/internal/**/*.ts"],
    "srcLink": "https://github.com/leonitousconforti/eftar/tree/main/src/",
    "examplesCompilerOptions": {
        "noEmit": true,
        "strict": true,
        "skipLibCheck": true,
        "moduleResolution": "Bundler",
        "module": "ES2022",
        "target": "ES2022",
        "lib": ["ES2022"],
        "baseUrl": "../../",
        "paths": {
            "eftar": ["./src/index.js"],
            "eftar/*": ["./src/*.js"]
        }
    }
}
