{
    "name": "eftar",
    "version": "0.0.63",
    "description": "GNU ustar tar implementation written using effect-ts",
    "keywords": [
        "tar",
        "gnu",
        "effect-ts"
    ],
    "homepage": "https://github.com/leonitousconforti/eftar",
    "bugs": {
        "url": "https://github.com/leonitousconforti/eftar/issues",
        "email": "leo@leoconforti.us"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/leonitousconforti/eftar.git"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": "./src/index.ts",
        "./*": "./src/*.ts",
        "./internal/*": null
    },
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/dts/index.d.ts",
    "directories": {
        "bin": "bin",
        "doc": "docs",
        "example": "examples",
        "test": "test"
    },
    "scripts": {
        "build": "pnpm clean && pnpm codegen && pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v3",
        "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
        "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
        "build-esm": "tsc -b tsconfig.build.json",
        "check": "tsc -b tsconfig.json",
        "circular": "node ./scripts/circular.mjs",
        "clean": "rimraf .tsbuildinfo docs build dist coverage",
        "codegen": "build-utils prepare-v3",
        "coverage": "vitest --coverage",
        "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
        "lint-fix": "pnpm lint --fix",
        "test": "vitest"
    },
    "devDependencies": {
        "@babel/cli": "7.27.2",
        "@babel/core": "7.27.1",
        "@babel/plugin-transform-export-namespace-from": "7.27.1",
        "@babel/plugin-transform-modules-commonjs": "7.27.1",
        "@changesets/cli": "2.29.3",
        "@effect/build-utils": "0.8.3",
        "@effect/cluster": "0.30.11",
        "@effect/docgen": "https://pkg.pr.new/Effect-TS/docgen/@effect/docgen@57ff897",
        "@effect/experimental": "0.44.21",
        "@effect/language-service": "0.10.1",
        "@effect/platform": "0.80.21",
        "@effect/platform-node": "0.77.11",
        "@effect/rpc": "0.56.9",
        "@effect/sql": "0.33.21",
        "@effect/vitest": "0.21.4",
        "@eslint/js": "9.26.0",
        "@types/node": "22.15.16",
        "@typescript-eslint/parser": "8.32.0",
        "@vitest/coverage-v8": "3.1.3",
        "babel-plugin-annotate-pure-calls": "0.5.0",
        "changesets-gitlab": "0.13.3",
        "effect": "3.14.21",
        "eslint": "9.26.0",
        "eslint-config-prettier": "10.1.3",
        "eslint-import-resolver-typescript": "4.3.4",
        "eslint-plugin-import-x": "4.11.0",
        "eslint-plugin-prettier": "5.4.0",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "eslint-plugin-sort-destructure-keys": "2.0.0",
        "glob": "11.0.2",
        "madge": "8.0.0",
        "prettier": "3.5.3",
        "prettier-plugin-jsdoc": "1.3.2",
        "prettier-plugin-packagejson": "2.5.11",
        "rimraf": "6.0.1",
        "tsx": "4.19.4",
        "typescript": "5.8.3",
        "typescript-eslint": "8.32.0",
        "vite": "6.3.5",
        "vitest": "3.1.3"
    },
    "peerDependencies": {
        "@effect/cluster": "0.30.11",
        "@effect/experimental": "0.44.21",
        "@effect/platform": "0.80.21",
        "@effect/rpc": "0.56.9",
        "@effect/sql": "0.33.21",
        "effect": "3.14.21"
    },
    "packageManager": "pnpm@9.15.9",
    "engines": {
        "node": ">=18.0.0"
    },
    "publishConfig": {
        "access": "public",
        "directory": "dist",
        "linkDirectory": false,
        "provenance": false
    },
    "pnpm": {
        "patchedDependencies": {
            "@effect/build-utils": "patches/@effect__build-utils.patch"
        }
    }
}
