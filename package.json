{
    "name": "eftar",
    "version": "0.0.0",
    "description": "GNU ustar tar implementation written using effect-ts",
    "keywords": [
        "tar",
        "gnu",
        "effect-ts"
    ],
    "homepage": "https://github.com/leonitousconforti/eftar",
    "bugs": {
        "url": "https://github.com/leonitousconforti/eftar/issues",
        "email": "leo@leoconforti.us"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/leonitousconforti/eftar.git"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/dts/index.d.ts",
    "directories": {
        "bin": "bin",
        "doc": "docs",
        "example": "examples",
        "test": "test"
    },
    "scripts": {
        "build": "pnpm clean && pnpm codegen && pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v2 && docgen",
        "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
        "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
        "build-esm": "tsc -b tsconfig.build.json",
        "check": "tsc -b tsconfig.json",
        "circular": "node ./scripts/circular.mjs",
        "clean": "rimraf .tsbuildinfo docs build dist coverage",
        "codegen": "build-utils prepare-v2",
        "coverage": "vitest --coverage",
        "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
        "lint-fix": "pnpm lint --fix",
        "test": "vitest"
    },
    "devDependencies": {
        "@babel/cli": "7.26.4",
        "@babel/core": "7.26.10",
        "@babel/plugin-transform-export-namespace-from": "7.25.9",
        "@babel/plugin-transform-modules-commonjs": "7.26.3",
        "@effect/build-utils": "0.7.9",
        "@effect/cli": "0.58.3",
        "@effect/docgen": "0.5.2",
        "@effect/eslint-plugin": "0.3.0",
        "@effect/experimental": "0.43.3",
        "@effect/language-service": "0.4.0",
        "@effect/platform": "0.79.3",
        "@effect/platform-node": "0.75.3",
        "@effect/printer": "0.41.12",
        "@effect/printer-ansi": "0.41.12",
        "@effect/typeclass": "0.32.12",
        "@effect/vitest": "0.19.10",
        "@eslint/compat": "1.2.7",
        "@eslint/eslintrc": "3.3.0",
        "@eslint/js": "9.22.0",
        "@types/node": "22.13.10",
        "@typescript-eslint/eslint-plugin": "8.26.1",
        "@typescript-eslint/parser": "8.26.1",
        "@typescript-eslint/utils": "8.26.1",
        "@vitest/coverage-v8": "3.0.9",
        "babel-plugin-annotate-pure-calls": "0.5.0",
        "effect": "3.13.12",
        "eslint": "9.22.0",
        "eslint-config-prettier": "10.1.1",
        "eslint-import-resolver-typescript": "4.2.1",
        "eslint-plugin-import-x": "4.9.0",
        "eslint-plugin-prettier": "5.2.3",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "eslint-plugin-sort-destructure-keys": "2.0.0",
        "glob": "11.0.1",
        "madge": "8.0.0",
        "prettier": "3.5.3",
        "prettier-plugin-jsdoc": "1.3.2",
        "prettier-plugin-packagejson": "2.5.10",
        "rimraf": "6.0.1",
        "tsx": "4.19.3",
        "typescript": "5.8.2",
        "typescript-eslint": "8.26.1",
        "vite": "6.2.2",
        "vitest": "3.0.9"
    },
    "peerDependencies": {
        "@effect/experimental": "0.43.3",
        "@effect/platform": "0.79.3",
        "@effect/platform-node": "0.75.3",
        "effect": "3.13.12"
    },
    "packageManager": "pnpm@9.15.9",
    "engines": {
        "node": ">=18.0.0"
    },
    "publishConfig": {
        "access": "public",
        "directory": "dist",
        "provenance": false
    },
    "pnpm": {
        "patchedDependencies": {
            "@effect/build-utils": "patches/@effect__build-utils.patch"
        }
    }
}
