{
    "name": "eftar",
    "version": "0.0.71",
    "description": "GNU ustar tar implementation written using effect-ts",
    "keywords": [
        "tar",
        "gnu",
        "effect-ts"
    ],
    "homepage": "https://github.com/leonitousconforti/eftar",
    "bugs": {
        "url": "https://github.com/leonitousconforti/eftar/issues",
        "email": "leo@leoconforti.us"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/leonitousconforti/eftar.git"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "sideEffects": [],
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": "./src/index.ts",
        "./*": "./src/*.ts",
        "./internal/*": null,
        "./*/index": null
    },
    "files": [
        "src/**/*.ts",
        "dist/**/*.js",
        "dist/**/*.js.map",
        "dist/**/*.d.ts",
        "dist/**/*.d.ts.map"
    ],
    "scripts": {
        "build": "pnpm clean && pnpm codegen && tsc -b tsconfig.build.json && babel dist --plugins annotate-pure-calls --out-dir dist --source-maps",
        "check": "tsc -b tsconfig.json",
        "circular": "madge --circular src/*.ts",
        "clean": "rimraf .tsbuildinfo docs build dist coverage",
        "codegen": "build-utils prepare-v4",
        "coverage": "vitest --coverage",
        "lint": "eslint \"**/{src,test,examples}/**/*.{ts,mjs}\"",
        "lint-fix": "pnpm lint --fix",
        "prepare": "effect-language-service patch",
        "test": "vitest"
    },
    "devDependencies": {
        "@babel/cli": "7.28.3",
        "@babel/core": "7.28.5",
        "@changesets/cli": "2.29.8",
        "@effect/build-utils": "0.8.9",
        "@effect/cluster": "0.54.0",
        "@effect/docgen": "https://pkg.pr.new/Effect-TS/docgen/@effect/docgen@49e99a2",
        "@effect/experimental": "0.57.8",
        "@effect/language-service": "0.57.1",
        "@effect/platform": "0.93.5",
        "@effect/platform-node": "0.102.0",
        "@effect/rpc": "0.72.2",
        "@effect/sql": "0.48.5",
        "@effect/vitest": "0.27.0",
        "@effect/workflow": "0.14.0",
        "@eslint/js": "9.39.1",
        "@types/node": "24.10.1",
        "@typescript-eslint/parser": "8.48.0",
        "@vitest/coverage-v8": "3.2.4",
        "babel-plugin-annotate-pure-calls": "0.5.0",
        "changesets-gitlab": "0.13.4",
        "effect": "3.19.8",
        "eslint": "9.39.1",
        "eslint-config-prettier": "10.1.8",
        "eslint-import-resolver-typescript": "4.4.4",
        "eslint-plugin-import-x": "4.16.1",
        "eslint-plugin-prettier": "5.5.4",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "eslint-plugin-sort-destructure-keys": "2.0.0",
        "glob": "13.0.0",
        "madge": "8.0.0",
        "prettier": "3.7.3",
        "prettier-plugin-jsdoc": "1.7.0",
        "prettier-plugin-packagejson": "2.5.20",
        "rimraf": "6.1.2",
        "tsx": "4.21.0",
        "typescript": "5.9.3",
        "typescript-eslint": "8.48.0",
        "vite": "7.2.4",
        "vite-tsconfig-paths": "5.1.4",
        "vitest": "3.2.4"
    },
    "peerDependencies": {
        "@effect/experimental": "0.57.8",
        "@effect/platform": "0.93.5",
        "effect": "3.19.8"
    },
    "packageManager": "pnpm@9.15.9",
    "engines": {
        "node": ">=18.0.0"
    },
    "publishConfig": {
        "access": "public",
        "exports": {
            "./package.json": "./package.json",
            ".": "./dist/index.js",
            "./*": "./dist/*.js",
            "./internal/*": null,
            "./*/index": null
        },
        "provenance": false
    }
}
